{
  "name": "storybook",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build-library": "webpack -p --config webpack.config.js --mode production",
    "build-storybook": "build-storybook -o build",
    "build-dev": "webpack -p --config webpack.config.js --mode development --watch",
    "storybook": "start-storybook -s ../build -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-dev": "webpack:serve --docker --dev-server-host=cli",
    "lint:css": "stylelint --ignore-path storybook/.eslintignore \"storybook/**/*.css\"",
    "lint:css:fix": "yarn run lint:css --fix",
    "lint:js": "eslint --ignore-path storybook/.eslintignore storybook/ --ext ts --ext js",
    "lint:js:fix": "yarn run lint:js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-rewrite": "./hooks/post-rewrite.sh ${HUSKY_GIT_PARAMS}"
    }
  },
  "lint-staged": {
    "*.{js,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@amazee/eslint-config-amazee": "^2.0.25",
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-notes": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/html": "^5.0.6",
    "@storybook/theming": "^5.1.9",
    "@types/node": "^11.13.0",
    "@types/storybook__html": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.0.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.0.3",
    "stylelint": "^9.10.1",
    "stylelint-config-amazee": "^1.0.3",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "tailwindcss": "^1.0.5",
    "twig": "^1.13.2",
    "twig-drupal-filters": "^2.0.0",
    "twig-loader": "https://github.com/AmazeeLabs/twig-loader.git",
    "typescript": "^3.4.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-fix-style-only-entries": "^0.3.0"
  },
  "resolutions": {
    "browserslist": "4.6.2",
    "caniuse-lite": "1.0.30000974"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  }
}
